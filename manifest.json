{
    "manifest_version": 2,
    "name": "Quick Bookmarks",
    "description": "Add a minified bookmarks toolbar to the context menu.",
    "version": "1.0",
    "homepage_url": "http://www.example.com",

    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "permissions": [
        "activeTab",
        "contextMenus",
		"bookmarks"
    ]
}
